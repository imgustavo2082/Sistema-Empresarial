@model Nomina.Models.Licencias

<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
<link href="https://stackpath.bootstrapcdn.com/bootswatch/4.3.1/sketchy/bootstrap.min.css" rel="stylesheet" integrity="sha384-N8DsABZCqc1XWbg/bAlIDk7AS/yNzT5fcKzg/TwfmTuUqZhGquVmpb5VvfmLcMzp" crossorigin="anonymous">

@{
    ViewBag.Title = "Details";
}

@{
    Layout = null;
}

@Html.Partial("NavBar")

<div class="jumbotron" align="center">
    <h2>Details</h2>

    <div>
        <h4>Licencias</h4>
        <p class="btn btn-link">
            @Html.ActionLink("Editar", "Edit", new { id = Model.id }) |
            @Html.ActionLink("Volver Atras", "Index")
        </p>
        <hr />
        <dl class="dl-horizonta table-primary">
            <dt>
                ID Empleado:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.idEmpleado)
            </dd>

            <dt>
                Fecha Inicio:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.fecha_inicio)
            </dd>

            <dt>
                Fecha Final:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.fecha_final)
            </dd>

            <dt>
                Motivo:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.motivo)
            </dd>

            <dt>
                Comentario:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.comentarios)
            </dd>

        </dl>
    </div>

</div>